<!DOCTYPE html>
<html>
  <head>
    <title>ResumeSite</title>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <!-- Bootstrap core CSS -->
    <link href="vendor/assets/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom fonts for this template -->
    <link href="https://fonts.googleapis.com/css?family=Saira+Extra+Condensed:100,200,300,400,500,600,700,800,900" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i" rel="stylesheet">
    <link href="vendor/assets/resume/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="vendor/assets/resume/devicons/css/devicons.min.css" rel="stylesheet">
    <link href="vendor/assets/resume/simple-line-icons/css/simple-line-icons.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="app/assets/stylesheets/resume.min.css" rel="stylesheet">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>


  </head>

  <body id="page-top">

    <%= yield %>

    <!-- Bootstrap core JavaScript -->
    <script src="vendor/assets/resume/jquery/jquery.min.js"></script>
    <script src="vendor/assets/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Plugin JavaScript -->
    <script src="vendor/assets/resume/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for this template -->
    <script src="app/assets/javascripts/resume.min.js"></script>
  </body>
</html>
